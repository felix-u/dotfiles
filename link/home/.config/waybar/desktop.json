{
    "layer": "top",
    "position": "top",
    "modules-left": [
        "river/tags",
        "cpu"
    ],
    "modules-right": [
        "pulseaudio",
        "custom/clock"
    ],
    "river/tags": {
        "num-tags": 9,
    },
    "cpu": {
      "interval": 2,
      "format": "CPU: {usage}%",
      "max-length": 10,
      "states": {
        "warning": 50,
        "critical": 80
      }
    },
    "pulseaudio": {
      "format": "{icon}   {volume}%",
      "format-icons": {
        "headphone": [ " ", " ", " ", " " ],
        "default": [ "", "", "", "" ],
        "bluetooth": [ " ", " ", " ", " " ],
      }
    }
}
